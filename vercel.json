{
    "version": 2,
    "builds": [
        {
            "src": "src/api/server.ts",
            "use": "@vercel/node"
        },
        {
            "src": "src/app/**/*.tsx",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/api/server.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/src/app/$1"
        }
    ]
}
